    "일렉트론",
    "일렉트론 리액트",
    "리액트 일렉트론",
    "일렉트론 샘플",
    "샘플 일렉트론",
    "일렉트론 예제",
    "예제 일렉트론",
    "일렉트론 예시",
    "예시 일렉트론",
    "electron 리액트",
    "일렉트론 리액트 예시",
    "일렉트론 리액트 예시 소스",
    "일렉트론 리액트 예시 코드",
    "일렉트론 리액트 예제",
    "일렉트론 리액트 예제 소스",
    "일렉트론 리액트 예제 코드",
    "react 일렉트론",
    "react",
    "electron 기본구조",
    "electron 기본 구조",
    "electron 구조",
    "electron 규격",
    "electron 폴더",
    "electron 폴더 구조",
    "electron folder",
    "electron folder structure",
    "electron structure"
  ],
  "homepage": "./",
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.40",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "app-root-path": "^3.1.0",
    "dotenv": "^16.3.1",
    "electron-is-dev": "^2.0.0",
    "electron-log": "^5.0.0-beta.25",
    "electron-updater": "^6.1.1",
    "env-cmd": "^10.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.15.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "react:start": "cross-env BROWSER=none react-scripts start",
    "react:build": "BUILD_PATH='./build/react' react-scripts build",
    "electron:start": "electron .",
    "electron:build": "tsc --project ./tsconfig.electron.json",
    "electron:react-wait:start": "wait-on http://127.0.0.1:3000 && npm run electron:start",
    "dev": "npm run del:build && npm run electron:build && npm run copy:env:dev && concurrently -n react,electron \"npm run react:start\" \"npm run electron:react-wait:start\"",
    "copy:env": "cpy ./.env ./build --dot",
    "copy:env:dev": "cpy ./.env.development ./build --dot --rename=.env",
    "del:build": "del-cli ./build",
    
    "build": "npm run del:build && npm run electron:build && npm run react:build && npm run copy:env && electron-builder build --config electron-builder-config.js",
    "build:dev": "npm run del:build && npm run electron:build && npm run react:build && npm run copy:env:dev && electron-builder build --config electron-builder-config-dev.js",

    "pack": "npm run build -- --publish never",
    "pack:deploy": "npm run build -- --publish always",
    "pack:dev": "npm run build:dev -- --publish never",
    "pack:dev:deploy": "npm run build:dev -- --publish always",
     
    "pack:win": "npm run build -- --win --publish never",
    "pack:win:deploy": "npm run build -- --win --publish always",
    "pack:win:dev": "npm run build:dev -- --win --publish never",
    "pack:win:dev:deploy": "npm run build:dev -- --win --publish always",

    "pack:mac": "npm run build -- --mac --publish never",
    "pack:mac:deploy": "npm run build -- --mac --publish always",
    "pack:mac:dev": "npm run build:dev -- --mac --publish never",
    "pack:mac:dev:deploy": "npm run build:dev -- --mac --publish always",

    "pack:linux": "npm run build -- --linux --publish never",
    "pack:linux:deploy": "npm run build -- --linux --publish always",
    "pack:linux:dev": "npm run build:dev -- --linux --publish never",
    "pack:linux:dev:deploy": "npm run build:dev -- --linux --publish always"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "cpy-cli": "^5.0.0",
    "cross-env": "^7.0.3",
    "del-cli": "^5.0.0",
    "electron": "^25.5.0",
    "electron-builder": "^24.6.3",
    "tailwindcss": "^3.3.3",
    "wait-on": "^7.0.1"
  },
  "main": "./build/electron.js"
}
